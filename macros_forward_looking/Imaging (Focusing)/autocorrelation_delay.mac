(dt) = autocorrelation_delay(sig1,sig2)
{
	  n=count(sig1);
	  y = 0;
	  xm = 0;
	  deltat = 9;
	  cross = push(ifft(fft(sig1)*conj(fft(sig2))),n/2);
	  y=max(cross,xm);
	  x1=xm ;y1=y;
	  x2=xm-deltat;

	y2=cross[x2];
	x3=xm+deltat;
		    
	y3=cross[x3];
	dt=-((x1*x1-x3*x3)*(y2-y3)-(x2*x2-x3*x3)*(y1-y3))/((y1-y3)*(x2-x3)-(y2-y3)*(x1-x3))/2-n/2;
	//out(dt);



}